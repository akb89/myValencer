{
  "name": "myValencer",
  "version": "1.0.14",
  "description": "A Valencer Pattern Search Engine for FrameNet: Web client for the Valencer API",
  "keywords": [
    "FrameNet",
    "Vue.js",
    "Search Engine",
    "Valence Pattern"
  ],
  "author": "YoosF",
  "license": "MIT",
  "main": "app/app.js",
  "homepage": "https://github.com/akb89/myvalencer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akb89/myvalencer.git"
  },
  "bugs": {
    "url": "https://github.com/akb89/myvalencer/issues"
  },
  "scripts": {
    "dev": "NODE_ENV=development node --harmony app/app.js",
    "build": "rm -rf public/* && NODE_ENV=production gulp build",
    "buildev": "NODE_ENV=development gulp default",
    "start": "NODE_ENV=production ./node_modules/.bin/pm2 start app/app.js --name myValencer --env production",
    "stop": "./node_modules/.bin/pm2 stop myValencer",
    "watch": "./node_modules/.bin/nodemon -e js,jsx,less,css,scss -w 'app/**/*.*' -w 'public/**/*.*' -x 'npm run dev'",
    "deploy": "npm install && npm run build && NODE_ENV=production ./node_modules/.bin/pm2 reload myValencer"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "bulma-accordion": "^2.0.0",
    "cytoscape": "^3.2.8",
    "cytoscape-cola": "^2.0.0",
    "cytoscape-cose-bilkent": "^4.0.0",
    "font-awesome": "^4.7.0",
    "jsdom": "^12.0.0",
    "kcors": "^2.2.1",
    "koa": "^2.3.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.2.1",
    "koa-send": "^5.0.0",
    "koa-views": "^6.0.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pm2": "^3.0.3",
    "superagent": "^3.8.3",
    "vue": "^2.5.x",
    "vue-infinite-loading": "^2.1.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "envify": "^4.1.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-vue": "^4.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-gzip": "^1.4.0",
    "gulp-html-replace": "^1.6.2",
    "gulp-if": "^2.0.2",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rev": "^8.1.1",
    "gulp-uglify-es": "^1.0.4",
    "gulp-sass": "^4.0.1",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "merge-stream": "^1.0.1",
    "mocha": "^5.0.0",
    "nodemon": "^1.18.3",
    "nyc": "^13.0.1",
    "rewire": "^4.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vueify": "^9.4.1"
  }
}

  "scripts": {
    "test": "npm run build && NODE_ENV=testing ./node_modules/.bin/mocha './tests/**/*.@(js)' --reporter spec",
    "cover": "npm run build && NODE_ENV=testing ./node_modules/.bin/istanbul cover _mocha -- ./tests/**/*.test.js",
    "build": "rm -rf build && flow-remove-types app/ -d build/ && cp -r ./public build/",
    "prepublish": "npm run build",
    "dev": "npm run build && node build/app.js",
    "start": "npm run build && NODE_ENV=production node build/app.js",
    "watch": "./node_modules/.bin/nodemon -e js,jsx,less,css,scss -w 'app/**/*.*' -w 'public/**/*.*' -x 'npm run dev'",
    "flow": "flow"
  },
